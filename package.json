  "dependencies": {
    "@fontsource/titan-one": "^5.2.5",
    "@reduxjs/toolkit": "^2.5.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.7.9",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.40.0",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.6.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0",
    "react-scripts": "5.0.1",
    "react-scroll-to-bottom": "^4.2.0",
    "redux": "^5.0.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^3.1.0",
    "remark-gfm": "^4.0.1",
    "socket.io-client": "^4.8.1",
    "swiper": "^11.2.5",
    "url-loader": "^4.1.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "serve -s dist --listen 80",
    "build": "cross-env NODE_ENV=production webpack --mode production --devtool source-map",
    "client": "cross-env NODE_ENV=development webpack-dev-server --mode development --devtool eval --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "clientshort": "webpack-dev-server --progress --inline --devtool eval --hot",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "copy-webpack-plugin": "^12.0.2",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.1.0",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0"
  }
